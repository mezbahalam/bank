<h2>Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :account %><br />
    <%= f.number_field :account%>
  </div>

    <div class="field">
    <%= f.label :birthday %><br />
    <%= f.date_select :birthday,:start_year => 1920, :end_year => 2000 %>
  </div>

  <div class="field">
    <%= f.label :password, "secret code" %><br />
    <%= f.password_field :password %>
  </div>


  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>

