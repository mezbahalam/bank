<h2>Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :account %><br />
    <%= f.number_field :account, autofocus: true %>
  </div>

    <div class="field">
      <%= f.label :birthday %><br />
      <%= f.date_select :birthday,order: [:month, :day, :year] %>
    </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>


  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>

